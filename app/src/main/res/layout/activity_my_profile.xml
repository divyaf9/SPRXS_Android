<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.MyProfileActivity">

    <TextView
        android:id="@+id/textView16"
        android:layout_width="412dp"
        android:layout_height="56dp"
        android:background="@color/colorAccent"
        android:gravity="bottom"
        android:paddingLeft="8dp"
        android:paddingBottom="8dp"
        android:text="PROFILE"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="315dp"
        android:background="@drawable/circle_background_orange"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_icons8_person" />

    <TextView
        android:id="@+id/nameProfileTextView"
        android:layout_width="275dp"
        android:layout_height="30dp"
        android:layout_marginStart="103dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="629dp"
        android:paddingLeft="8dp"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/mailProfileTextView"
        android:layout_width="275dp"
        android:layout_height="30dp"
        android:layout_marginStart="103dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="33dp"
        android:paddingLeft="8dp"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="412dp"
        android:layout_height="56dp"
        android:layout_marginTop="300dp"
        android:layout_marginBottom="502dp"
        android:background="@color/colorAccent"
        android:gravity="bottom"
        android:paddingLeft="8dp"
        android:paddingBottom="8dp"
        android:text="LOGOUT"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/logoutTextView"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="365dp"
        android:layout_marginBottom="445dp"
        android:gravity="center"
        android:text="Log out"
        android:textStyle="bold"
        android:textColor="#F44336"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="412dp"
        android:layout_height="56dp"
        android:layout_marginTop="413dp"
        android:layout_marginBottom="362dp"
        android:background="@color/colorAccent"
        android:gravity="bottom"
        android:paddingLeft="8dp"
        android:paddingBottom="8dp"
        android:text="POLICIES"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/privacyProfileTextView"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="505dp"
        android:layout_marginBottom="305dp"
        android:gravity="center"
        android:text="Privacy Policy"
        android:textColor="#2196F3"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/competitionTextView"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="539dp"
        android:layout_marginBottom="271dp"
        android:gravity="center"
        android:text="Competition Rules"
        android:textColor="#2196F3"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>

